<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient
                    android:angle="45"
                    android:endColor="#2200cc33"
                    android:startColor="#2200cc33"></gradient>
                <corners android:radius="4dp"></corners>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:angle="45"
                    android:endColor="#00cc33"
                    android:startColor="#00cc33"></gradient>
                <corners android:radius="4dp"></corners>
            </shape>
        </clip>
    </item>
</layer-list>